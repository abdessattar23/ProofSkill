{
    "name": "proofskill-backend",
    "version": "0.1.0",
    "type": "module",
    "private": true,
    "scripts": {
        "dev": "tsx watch src/server.ts",
        "build": "tsc -p tsconfig.json",
        "start": "node dist/server.js",
        "lint": "eslint . --ext .ts",
        "smoke": "node scripts/smoke.cjs",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:unit": "vitest run tests/",
        "test:integration": "vitest run tests/integration.test.ts",
        "load-test": "tsx scripts/loadTest.ts",
        "db:migrate": "tsx src/cli/db.ts migrate",
        "db:rollback": "tsx src/cli/db.ts rollback",
        "db:status": "tsx src/cli/db.ts status",
        "db:create": "tsx src/cli/db.ts create",
        "db:seed:skills": "tsx src/cli/db.ts seed:skills",
        "setup": "node scripts/setup.mjs",
        "migrate": "node scripts/migrate.mjs"
    },
    "dependencies": {
        "@fastify/cors": "^9.0.1",
        "@fastify/jwt": "^5.0.0",
        "@fastify/multipart": "^8.3.0",
        "@fastify/rate-limit": "^9.1.0",
        "@fastify/sensible": "^5.6.0",
        "@fastify/swagger": "^9.4.0",
        "@fastify/swagger-ui": "^3.0.0",
        "@google/generative-ai": "^0.24.1",
        "@supabase/storage-js": "^2.10.4",
        "@supabase/supabase-js": "^2.43.4",
        "@types/bcrypt": "^6.0.0",
        "@types/jsonwebtoken": "^9.0.10",
        "axios": "^1.11.0",
        "bcrypt": "^6.0.0",
        "commander": "^14.0.0",
        "dotenv": "^16.4.5",
        "fastify": "^4.28.1",
        "form-data": "^4.0.4",
        "ioredis": "^5.4.1",
        "jsonwebtoken": "^9.0.2",
        "mammoth": "^1.6.0",
        "pdf-parse": "^1.1.1",
        "pino": "^9.3.2",
        "prom-client": "^15.1.3",
        "uuid": "^9.0.1",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@types/node": "^20.14.9",
        "@types/pdf-parse": "^1.1.4",
        "@typescript-eslint/eslint-plugin": "^7.17.0",
        "@typescript-eslint/parser": "^7.17.0",
        "eslint": "^8.57.0",
        "pino-pretty": "^11.2.2",
        "tsx": "^4.16.2",
        "typescript": "^5.5.4",
        "vitest": "^1.6.0"
    }
}